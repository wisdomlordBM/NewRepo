@model IEnumerable<dynamic>

<h2>Applications</h2>
<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Location</th>
            <th>Contact</th>
            <th>Description</th>
            <th>Date Applied</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var application in Model)
        {
            <tr>
                <td>@application.Name</td>
                <td>@application.Email</td>
                <td>@application.Location</td>
                <td>@application.Contact</td>
                <td>@application.Description</td>
                <td>@application.DateApplied.ToString("g")</td>
            </tr>
        }
    </tbody>
</table>
